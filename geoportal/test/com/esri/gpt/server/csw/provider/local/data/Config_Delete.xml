    <xml><![CDATA[
      <?xml version="1.0" encoding="UTF-8"?>
      <csw:Transaction xmlns:csw="http://www.opengis.net/cat/csw/2.0.2" service="CSW">
        <csw:Delete handle="hdl"></csw:Delete>
      </csw:Transaction>
    ]]></xml> 
