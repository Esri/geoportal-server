<?xml version="1.0" encoding="utf-8"?>
<Configuration Version="1.10" InheritFromDefault="false">

	<Globals xmlns="http://www.geocortex.com/Viewer/Configuration/PreProcessor">
	
	<Insert__Toolbar-SearchGeoportal-Button>
		<Button Name="Search Geoportal"
				Command="PortalSearchCommand"
				ImageUri="/Resources/Images/GeoportalSearch.png"
				ToolTip="Search Geoportal"
				DisplayName="Search Geoportal" />
	</Insert__Toolbar-SearchGeoportal-Button>
	
	</Globals>
	
	<Libraries>
		<Library Id="GDA.GE.SL.Modules"
		           Uri="GDA.GE.SL.Modules.xap"
				   AssemblyName="GDA.GE.SL.Modules, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" 
				   ClassNameTemplate="GDA.GE.SL.Modules.Modules.{_ModuleName}.{_ModuleName}{_Type}" />
	</Libraries>

	<Modules>
    <GeoportalSearch LibraryId="GDA.GE.SL.Modules">
      <Views>
        <View Id="GeoportalSearchView" Region="DataRegion">
        </View>
      </Views>
      <Configuration>
        <ServiceHost Value="mapservicehost.com" Proxy="../../proxy.ashx" />
      </Configuration>
    </GeoportalSearch>
	</Modules>
</Configuration>
